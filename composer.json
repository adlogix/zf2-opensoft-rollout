{
  "name": "adlogix/zf2-opensoft-rollout",
  "description": "ZF2 module for the Opensoft Rollout library",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "zf2",
    "feature flags",
    "feature",
    "flags"
